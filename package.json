{
  "name": "roots-util",
  "version": "0.2.0",
  "author": "Jeff Escalante <hello@jenius.me>",
  "description": "a utility for building roots extensions",
  "license": "MIT",
  "keywords": [
    "roots",
    "extension",
    "utility"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/carrot/roots-util.git"
  },
  "main": "lib",
  "dependencies": {
    "colors": "1.x",
    "glob": "7.0.5",
    "lodash": "4.15.0",
    "minimatch": "3.x",
    "mkdirp": "0.5.x",
    "rimraf": "2.x",
    "vinyl": "1.2.0",
    "when": "3.x"
  },
  "devDependencies": {
    "coffee-script": "1.10.x",
    "coveralls": "2.x",
    "istanbul": "0.4.x",
    "mocha": "2.5.0",
    "mocha-lcov-reporter": "1.2.0",
    "roots": "5.0.0",
    "should": "11.1.0"
  },
  "scripts": {
